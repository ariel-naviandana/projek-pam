<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HeadActivity">

    <ImageView
        android:id="@+id/imgMuka"
        android:layout_width="272dp"
        android:layout_height="398dp"
        android:layout_marginStart="69dp"
        android:layout_marginTop="105dp"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="203dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/body" />

    <ImageView
        android:id="@+id/imgRambut"
        android:layout_width="274dp"
        android:layout_height="162dp"
        android:layout_marginStart="67dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="70dp"
        app:layout_constraintBottom_toTopOf="@+id/imgMuka"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.267"
        app:srcCompat="@drawable/hair" />

    <ImageView
        android:id="@+id/imgAlis"
        android:layout_width="207dp"
        android:layout_height="48dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/eyebrow" />

    <ImageView
        android:id="@+id/imgMata"
        android:layout_width="137dp"
        android:layout_height="132dp"
        android:layout_marginEnd="136dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgRambut"
        app:srcCompat="@drawable/eyes" />

    <ImageView
        android:id="@+id/imgJanggut"
        android:layout_width="188dp"
        android:layout_height="143dp"
        android:layout_marginStart="112dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgMata"
        app:srcCompat="@drawable/beard" />

    <ImageView
        android:id="@+id/imgKumis"
        android:layout_width="243dp"
        android:layout_height="80dp"
        android:layout_marginEnd="84dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgMata"
        app:srcCompat="@drawable/moustache" />

    <CheckBox
        android:id="@+id/cbRambut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="114dp"
        android:checked="true"
        android:clickable="true"
        android:text="Rambut"
        app:layout_constraintEnd_toStartOf="@+id/cbAlis"
        app:layout_constraintTop_toBottomOf="@+id/imgMuka" />

    <CheckBox
        android:id="@+id/cbAlis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="103dp"
        android:checked="true"
        android:clickable="true"
        android:text="Alis"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgMuka" />

    <CheckBox
        android:id="@+id/cbKumis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="121dp"
        android:checked="true"
        android:clickable="true"
        android:text="Kumis"
        app:layout_constraintEnd_toStartOf="@+id/cbJanggut"
        app:layout_constraintTop_toBottomOf="@+id/cbRambut" />

    <CheckBox
        android:id="@+id/cbJanggut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="74dp"
        android:checked="true"
        android:clickable="true"
        android:text="Janggut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cbAlis" />
</androidx.constraintlayout.widget.ConstraintLayout>